
  <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
  <script type="text/javascript">
      google.charts.load('current', {'packages':['line']});
      google.charts.setOnLoadCallback(drawChart);

    function drawChart() {

      var data = new google.visualization.DataTable();
      var coll_ids = <%== @data[:coll_ids] %>
      data.addColumn('number', 'Day');
      console.log(coll_ids)
      for (let step = 0; step < coll_ids.length; step++) {
            data.addColumn('number', coll_ids[step]);
        
          }
      data.addRows(<%=raw(@data[:data])%>);

      var options = {
        chart: {
          title: '<%= raw(@data[:title]) %>',
          
        },
        width: 900,
        height: 500,
        axes: {
          x: {
            0: {side: 'top'}
          }
        }
      };

      var chart = new google.charts.Line(document.getElementById('line_top_x'));

      chart.draw(data, google.charts.Line.convertOptions(options));
    }
  </script>
<h5 class='text-center' style='font-family:Verdana;color:white;font-size:2rem;'>
  <%= @data[:title] %>
</h5>
<div class="container d-flex justify-content-center">
  
  <div id="line_top_x">
  </div>
</div>
<div class='d-flex justify-content-center '>
    <a href=<%=  vote_url(votes_id: @data[:votes_id]) %> class = 'back_vote_stats'>
        <img src="https://i.imgur.com/AvW7QuM.png" alt="" class='img-fluid imagen_vote_show'>
    </a>
</div>

  

